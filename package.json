{
  "name": "skype-http",
  "description": "Unofficial Skype API for Node.js via 'Skype (HTTP)'",
  "version": "0.0.4",
  "license": "MIT",
  "main": "./dist/src/skyweb.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/skype-http.git"
  },
  "scripts": {
    "build": "tsc -p src/lib",
    "clean": "node tools/clean.js",
    "lint": "tslint \"src/**/*.ts\"",
    "prepublish": "npm run lint && npm run clean && npm run build && npm run test && node tools/prepublish.js",
    "test": "tsc -p src/test && mocha build/test --recursive"
  },
  "keywords": [
    "Skype",
    "Skype-Web",
    "Skype-HTTP"
  ],
  "dependencies": {
    "big-integer": "^1.5.6",
    "cheerio": "^0.19.0",
    "es6-promise": "^3.0.2",
    "js-sha256": "^0.3.0",
    "request": "2.61",
    "tough-cookie": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "fs-extra": "^0.30.0",
    "mocha": "^2.4.5",
    "tslint": "^3.9.0",
    "typescript": "^1.8.10"
  }
}
